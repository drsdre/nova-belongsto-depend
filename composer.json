{
    "name": "emilianotisato/belongsto-depends",
    "description": "A Laravel Nova field with dependencie on another BelongsTo relationship.",
    "keywords": [
        "laravel",
        "nova",
        "belongsTo relationship",
        "fields depend"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "EmilianoTisato\\NovaBelongsToDepends\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EmilianoTisato\\NovaBelongsToDepends\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}